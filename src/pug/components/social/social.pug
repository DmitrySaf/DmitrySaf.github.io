mixin social 
    .social 
        a.social_twitter(href="http://twitter.com/")
            img(src="./assets/icons/twitter.svg", alt="twitter")
        a.social_facebook(href="http://www.facebook.com/")
            img(src="./assets/icons/facebook.svg", alt="facebook")
        a.social_instagram(href="http://instagram.com/")
            img(src="./assets/icons/instagram.svg", alt="instagram")